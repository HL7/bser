    <context value="Condition" />
    <type value="Condition" />
    <baseDefinition value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-condition" />
    <derivation value="constraint" />
    <differential>
        <element id="Condition">
            <path value="Condition" />
        </element>
        <element id="code">
            <path value="Condition.code" />
            <short value="code" />
            <definition value="SHALL contain exactly one [1..1] code (CONF:4383-314)." />
            <min value="1" />
            <max value="1" />
        </element>
        <element id="code.coding">
            <path value="Condition.code.coding" />
            <short value="coding" />
            <definition value="This code SHALL contain exactly one [1..1] coding (CONF:4383-315)." />
            <min value="1" />
            <max value="1" />
        </element>
        <element id="code.coding.code">
            <path value="Condition.code.coding.code" />
            <short value="code" />
            <definition value="This coding SHALL contain exactly one [1..1] code (CONF:4383-318)." />
            <min value="1" />
            <max value="1" />
        </element>
        <element id="code.coding.display">
            <path value="Condition.code.coding.display" />
            <short value="display" />
            <definition value="This coding SHOULD contain zero or one [0..1] display (CONF:4383-319)." />
            <min value="0" />
            <max value="1" />
        </element>
        <element id="subject">
            <path value="Condition.subject" />
            <short value="subject" />
            <definition value="SHALL contain exactly one [1..1] BSeR Patient (identifier: http://hl7.org/fhir/us/bser/StructureDefinition/PatientRole) (CONF:4383-316)." />
            <min value="1" />
            <max value="1" />
            <type>
                <code value="Reference" />
                <targetProfile value="http://hl7.org/fhir/us/bser/StructureDefinition/PatientRole" />
            </type>
        </element>
        <element id="onset[x]">
            <path value="Condition.onset[x]" />
            <slicing>
                <discriminator>
                    <type value="value" />
                    <path value="@type" />
                </discriminator>
                <rules value="open" />
            </slicing>
            <short value="onset[x]" />
            <definition value="SHOULD contain zero or one [0..1] onset[x], where onset[x] is one of (CONF:4383-317)" />
            <min value="0" />
            <max value="1" />
        </element>
        <element id="onset[x].onsetDateTime">
            <path value="Condition.onsetDateTime" />
            <short value="onsetDateTime" />
            <definition value="onsetDateTime (CONF:4383-320)" />
        </element>
        <element id="onset[x].onsetAge">
            <path value="Condition.onsetAge" />
            <short value="onsetAge" />
            <definition value="or onsetAge (CONF:4383-321)" />
        </element>
        <element id="onset[x].onsetString">
            <path value="Condition.onsetString" />
            <short value="onsetString" />
            <definition value="or onsetString (CONF:4383-322)" />
        </element>
    </differential>
